{
    "containerDefinitions": [
        {
            "name": "spring-web-app",
            "image": "851725302967.dkr.ecr.us-east-1.amazonaws.com/spring-boot-eks-repo:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "spring-web-app-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                },
                {
                    "name": "spring-web-app-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/spring-boot-eks-task",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "family": "spring-boot-eks-task",
    "taskRoleArn": "arn:aws:iam::851725302967:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::851725302967:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "revision": 5,
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}